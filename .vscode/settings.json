{
  "editor.defaultFormatter": "biomejs.biome",
  "[css][scss][jsonc][json][typescript][typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "editor.rulers": [
    100
  ],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports.biome": "explicit",
    "source.addMissingImports": "explicit"
  },
  "search.exclude": {
    "**/dist": true,
    "dist": true,
    "static/tradingview-lib": true,
    "out": true,
    "**/.nuxt": true,
    "**/.next": true,
    ".next": true,
    "**/coverge": true,
    "node_modules": true,
    "packages/**/.next": true,
    "**/node_modules": true,
    "**/yarn.lock": true,
    "**/*.code-search": true,
    "packages/**/node_modules": true,
    "**/bower_components": true,
    "ios": true,
    ".yarn": true,
    "android": true,
    "fastlane": true,
    "build": true,
    "coverage": true,
    ".idea": true,
    ".dart_tool": true,
    ".flutter-plugins": true,
    ".flutter-plugins-dependencies": true,
    ".packages": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "ios": true,
    "android": true,
    "fastlane": true,
    ".idea": true,
    ".dart_tool": true,
    ".flutter-plugins": true,
    ".flutter-plugins-dependencies": true,
    ".packages": true
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always"
}
